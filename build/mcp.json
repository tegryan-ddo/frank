{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@context7/mcp-server"],
      "env": {}
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "env": {}
    },
    "aws-documentation": {
      "command": "uvx",
      "args": ["mcp-server-aws-documentation"],
      "env": {}
    },
    "aws-knowledge": {
      "command": "uvx",
      "args": ["awslabs.amazon-q-developer-mcp-server"],
      "env": {}
    },
    "aws-core": {
      "command": "uvx",
      "args": ["mcp-server-aws"],
      "env": {
        "AWS_PROFILE": "${AWS_PROFILE}",
        "AWS_REGION": "${AWS_REGION}"
      }
    }
  }
}
